{
    "manifest_version": 2,
    "name":"Tab Saver",
    "description": "save your RAM by making a convenient list out of all open tabs",
    "version": "0.2",
    "browser_action":{
        "default_icon": "icon128.png",
        "default_popup":"popup.html"
    },
    "permissions":[
        "tabs"
    ]
}